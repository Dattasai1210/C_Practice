#include<stdio.h>
int main()
{
    int arr[]={1,2,3,4,5};
    int pos=2;
    for(int i=0;i<5;i++)
    {
        arr[pos]=10;
        printf("%d\t",arr[i]);
    }
    printf("\n");
     pos=1;
    for(int i=1;i<5;i++)
    {
        arr[i]=arr[i+1];
    }
    for(int i=0;i<5;i++)
    {
    printf("%d\t",arr[i]);
    }
}

Output;
1       2       10      4       5
1       10      4       5       0
